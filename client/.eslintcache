om\";\r\nimport isEmpty from \"validator/lib/isEmpty\";\r\nimport isEmail from \"validator/lib/isEmail\";\r\nimport equals from \"validator/lib/equals\";\r\nimport \"./Signup.css\";\r\nimport { showErrorMsg, showSuccessMsg } from \"../../helpers/message\";\r\nimport { showLoading } from \"../../helpers/loading\";\r\nimport { signup } from \"../../api/auth\";\r\nconst Signup = () => {\r\n  const [formData, setFormData] = useState({\r\n    username: \"johndoe\",\r\n    email: \"jdoe@gmail.com\",\r\n    password: \"abc123\",\r\n    password2: \"abc123\",\r\n    successMsg: false,\r\n    errorMsg: false,\r\n    loading: false,\r\n  });\r\n  const {\r\n    username,\r\n    email,\r\n    password,\r\n    password2,\r\n    successMsg,\r\n    errorMsg,\r\n    loading,\r\n  } = formData;\r\n  /****************************\r\n   * EVENT HANDLERS\r\n   ***************************/\r\n  const handleChange = (evt) => {\r\n    //console.log(evt);\r\n    setFormData({\r\n      ...formData,\r\n      [evt.target.name]: evt.target.value,\r\n      /* so that the error messages disappear as the user starts typing */\r\n      successMsg: \"\",\r\n      errorMsg: \"\",\r\n    });\r\n  };\r\n\r\n  const handleSubmit = (evt) => {\r\n    evt.preventDefault();\r\n\r\n    // client-side validation\r\n    if (\r\n      isEmpty(username) ||\r\n      isEmpty(email) ||\r\n      isEmpty(password) ||\r\n      isEmpty(password2)\r\n    ) {\r\n      setFormData({\r\n        ...formData,\r\n        errorMsg: \"All fields are required\",\r\n      });\r\n    } else if (!isEmail(email)) {\r\n      setFormData({\r\n        ...formData,\r\n        errorMsg: \"Invalid email\",\r\n      });\r\n    } else if (!equals(password, password2)) {\r\n      setFormData({\r\n        ...formData,\r\n        errorMsg: \"Passwords do not match\",\r\n      });\r\n    } else {\r\n      const { username, email, password } = formData;\r\n      const data = { username, email, password };\r\n\r\n      setFormData({ ...formData, loading: true });\r\n      //send data to server\r\n      signup(data)\r\n        .then((response) => {\r\n          console.log(\"Axios signup success: \", response);\r\n          setFormData({\r\n            username: \"\",\r\n            email: \"\",\r\n            password: \"\",\r\n            password2: \"\",\r\n            loading: false,\r\n            successMsg: response.data.successMessage,\r\n          });\r\n        })\r\n        .catch((err) => {\r\n          console.log(\"Axios signup error: \", err);\r\n          setFormData({\r\n            ...formData,\r\n            loading: false,\r\n            errorMsg: err.response.data.errorMessage,\r\n          });\r\n        });\r\n    }\r\n  };\r\n\r\n  /****************************\r\n   * VIEWS\r\n   ***************************/\r\n  const showSignupForm = () => (\r\n    <form className=\"signup-form\" onSubmit={handleSubmit} noValidate>\r\n      {/* username */}\r\n      <div className=\"form-group input-group\">\r\n        <div className=\"input-group-prepend\">\r\n          <span className=\"input-group-text\">\r\n            <i className=\"fa fa-user\"></i>\r\n          </span>\r\n        </div>\r\n        <input\r\n          name=\"username\"\r\n          value={username}\r\n          className=\"form-control\"\r\n          placeholder=\"Username\"\r\n          type=\"text\"\r\n          onChange={handleChange}\r\n        />\r\n      </div>\r\n      {/* email */}\r\n      <div className=\"form-group input-group\">\r\n        <div className=\"input-group-prepend\">\r\n          <span className=\"input-group-text\">\r\n            <i className=\"fa fa-envelope\"></i>\r\n          </span>\r\n        </div>\r\n        <input\r\n          name=\"email\"\r\n          value={email}\r\n          className=\"form-control\"\r\n          placeholder=\"Email address\"\r\n          type=\"email\"\r\n          onChange={handleChange}\r\n        />\r\n      </div>\r\n      {/* password */}\r\n      <div className=\"form-group input-group\">\r\n        <div className=\"input-group-prepend\">\r\n          <span className=\"input-group-text\">\r\n            <i className=\"fa fa-lock\"></i>\r\n          </span>\r\n        </div>\r\n        <input\r\n          name=\"password\"\r\n          value={password}\r\n          className=\"form-control\"\r\n          placeholder=\"Create password\"\r\n          type=\"password\"\r\n          onChange={handleChange}\r\n        />\r\n      </div>\r\n      {/* password2 */}\r\n      <div className=\"form-group input-group\">\r\n        <div className=\"input-group-prepend\">\r\n          <span className=\"input-group-text\">\r\n            <i className=\"fa fa-lock\"></i>\r\n          </span>\r\n        </div>\r\n        <input\r\n          name=\"password2\"\r\n          value={password2}\r\n          className=\"form-control\"\r\n          placeholder=\"Confirm password\"\r\n          type=\"password\"\r\n          onChange={handleChange}\r\n        />\r\n      </div>\r\n      {/* signup button */}\r\n      <div className=\"form-group\">\r\n        <button type=\"submit\" className=\"btn btn-primary btn-block\">\r\n          Signup\r\n        </button>\r\n      </div>\r\n      {/* already have account */}\r\n      <p className=\"text-center text-white\">\r\n        Have an account? <Link to=\"/signin\">Log In</Link>\r\n      </p>\r\n    </form>\r\n  );\r\n\r\n  /****************************\r\n   * RENDERER\r\n   ***************************/\r\n  return (\r\n    <div className=\"signup-container\">\r\n      <div className=\"row px-3 vh-100\">\r\n        <div className=\"col-md-5 mx-auto align-self-center\">\r\n          {successMsg && showSuccessMsg(successMsg)}\r\n          {errorMsg && showErrorMsg(errorMsg)}\r\n          {loading && <div className=\"text-center pb-4\">{showLoading()}</div>}\r\n          {showSignupForm()}\r\n          {/* <p style={{ color: 'white' }}>{JSON.stringify(formData)}</p> */}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Signup;\r\n","C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\components\\Login\\Login.js",[],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\components\\Home\\Home.js",[],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\helpers\\message.js",[],["76","77"],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\helpers\\loading.js",[],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\api\\auth.js",[],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\helpers\\auth.js",[],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\helpers\\localStorage.js",[],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\helpers\\cookies.js",[],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\components\\Admin\\AdminDashboard.js",[],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\components\\User\\UserDashboard.js",[],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\components\\ProtectedRoutes\\AdminRoute.js",[],"C:\\Users\\FIDELLIS\\desktop\\coding\\mern-projects\\mern-boilerplate1\\client\\src\\components\\ProtectedRoutes\\UserRoute.js",[],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":2,"column":16,"nodeType":"84","messageId":"85","endLine":2,"endColumn":26},{"ruleId":"78","replacedBy":"86"},{"ruleId":"80","replacedBy":"87"},"no-native-reassign",["88"],"no-negated-in-lhs",["89"],"no-unused-vars","'useHistory' is defined but never used.","Identifier","unusedVar",["88"],["89"],"no-global-assign","no-unsafe-negation"]